{
  "name": "code-worker",
  "displayName": "code-worker",
  "description": "quickfix,postfix.(like intellij).",
  "version": "0.0.8",
  "publisher": "fzdwx",
  "repository": {
    "url": "https://github.com/fzdwx/code-worker"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:rust",
    "onLanguage:go",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:java",
    "onLanguage:c",
    "onLanguage:markdown",
    "onFileSystem:config.yml",
    "onFileSystem:config.yml"
  ],
  "extensionPack": [
    "antfu.browse-lite"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hugo.stop",
        "title": "Stop hugo Server"
      },
      {
        "command": "hugo.restart",
        "title": "Start hugo Server"
      },
      {
        "command": "hugo.open",
        "title": "Open hugo app"
      },
      {
        "command": "hugo.showCommands",
        "title": "Show commands",
        "icon": "$(symbol-event)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.73.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.2.0"
  }
}